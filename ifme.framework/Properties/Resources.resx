  <data name="ScriptUpdateLinux" xml:space="preserve">
    <value>#!/bin/sh
# Usage:
#   update.sh "parm 1" "parm 2" "parm 3"
# example:
#   update.sh "http://update.example.com/myapp/1.22/file.zip" "/home/me/Desktop/myapp" "myapp.sh"
chmod +x ./7za
echo " "
echo "This terminal will perform IFME update to the latest version."
echo "Please be patient this may take a while depending your"
echo "internet configuration, you can do other thing while updating."
echo " "
sleep 5
wget --no-check-certificate -O "./saishin.jp" $1
./7za x -so "./saishin.jp" | tar --overwrite -xf - -C "$2"
echo " "
echo " "
echo "Update complete!"
echo "IFME will start after 3 seconds..."
sleep 3
cd "$2"
sh $3</value>
  </data>
  <data name="ScriptUpdateWindows" xml:space="preserve">
    <value>@echo off
@title IFME Updater
REM Usage:
REM   update.cmd "parm 1" "parm 2" "parm 3"
REM example:
REM   update.cmd "http://update.example.com/myapp/1.22/file.zip" "C:\Program Files\myapp" "myapp.exe"
echo.
echo This console will perfom IFME update to the latest version.
echo Please be patient this may take a while depending your
echo internet configuration, you can do other thing while updating.
echo.
TIMEOUT /T 5 /NOBREAK
copy %2\unins000.exe .\unins000.exe
copy %2\unins000.dat .\unins000.dat
wget --no-check-certificate -O .\saishin.jp %1
7za x -y -o%2 .\saishin.jp
copy .\unins000.exe %2\unins000.exe
copy .\unins000.dat %2\unins000.dat
echo.
echo.
@title Update Complete!
echo IFME will start after 3 second...
echo.
TIMEOUT /T 3 /NOBREAK
cd %2
start "IFME" /D %2 %2\%3
exit</value>
